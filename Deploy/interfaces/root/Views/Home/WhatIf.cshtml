
@model DBApp.Models.DataBag

@{
    ViewBag.Title = "Results";
}


<div class="row">
    <div class="col-md-6">
        <h2>Results</h2>
        <p>
           New Balance: @Model.WhatIfResults.NewBalance
        </p>
        <p>
           Deposit: @Model.WhatIfResults.Deposit
        </p>
        <p>
           Original balance: @Model.WhatIfResults.Balance
        </p>
        <p><img src="@Url.Action("WhatIfResults")" /> </p>
        <p><a href="/" class="btn btn-primary btn-lg">Back &raquo;</a></p>
    </div>
    
</div>